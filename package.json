{
  "name": "strumble-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --webpack",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "eslint",
    "db:inspect": "atlas schema inspect --env supabase",
    "db:inspect:local": "atlas schema inspect --env local",
    "db:diff": "atlas schema diff --env supabase --to file://atlas/schema.sql",
    "db:apply": "atlas schema apply --env supabase --to file://atlas/schema.sql --auto-approve",
    "db:apply:dry": "atlas schema apply --env supabase --to file://atlas/schema.sql --dry-run",
    "db:apply:local": "atlas schema apply --env local --to file://atlas/schema.sql --auto-approve",
    "db:migrate:new": "atlas migrate diff --env supabase",
    "db:migrate:apply": "atlas migrate apply --env supabase",
    "db:migrate:apply:local": "atlas migrate apply --env local",
    "db:migrate:status": "atlas migrate status --env supabase",
    "db:migrate:lint": "atlas migrate lint --env supabase",
    "db:migrate:hash": "atlas migrate hash --dir file://atlas/migrations",
    "db:migrate:validate": "atlas migrate validate --env supabase",
    "db:seed": "sh -c 'psql \"$SUPABASE_DB_URL\" -f atlas/seed.sql'",
    "db:policies": "sh -c 'psql \"$SUPABASE_DB_URL\" -f atlas/policies.sql'",
    "db:seed:local": "psql postgresql://postgres:postgres@localhost:54322/postgres -f atlas/seed.sql",
    "db:functions": "sh -c 'psql \"$SUPABASE_DB_URL\" -f atlas/functions.sql'",
    "db:functions:local": "psql postgresql://postgres:postgres@localhost:54322/postgres -f atlas/functions.sql",
    "db:reset:local": "supabase db reset",
    "db:docs": "atlas schema doc --env supabase",
    "db:setup": "npm run db:apply && npm run db:functions && npm run db:seed",
    "db:setup:local": "npm run db:apply:local && npm run db:functions:local && npm run db:seed:local"
  },
  "dependencies": {
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.78.0",
    "@tanstack/react-query": "^5.90.5",
    "chart.js": "^4.5.1",
    "maplibre-gl": "^5.10.0",
    "next": "16.0.0",
    "react": "19.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "19.2.0",
    "react-map-gl": "^8.1.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "16.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16",
    "typescript": "^5"
  }
}
